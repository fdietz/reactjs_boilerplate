{
  "name": "reactjs_boilerplate",
  "version": "1.0.0",
  "description": "reactjs boilerplate",
  "scripts": {
    "dev:compile": "rimraf dist && webpack --progress --colors",
    "prod:compile": "rimraf dist && set NODE_ENV=production && webpack -p --progress --colors",
    "start": "webpack-dev-server --progress --hot --inline",
    "lint": "eslint . --ext .js --ignore-path .gitignore --cache"
  },
  "author": "Frederik Dietz",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^4.1.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
